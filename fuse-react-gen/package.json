{
  "name": "@fuselab/react-gen",
  "version": "1.2.3",
  "description": "code gen tool for react.js in typescript",
  "main": "./lib/index.js",
  "bin": {
    "swagger-ts": "./lib/index.js"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "build-all": "tsc",
    "tslint": "tslint -c tslint.json -t stylish --project tsconfig.json"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/*.spec.(ts|tsx)"
    ],
    "testPathIgnorePatterns": [
      "/examples/"
    ],
    "collectCoverage": true,
    "coverageDirectory": "./coverage"
  },
  "dependencies": {
    "@fuselab/ui-shared": "^1.1.0",
    "glob": "^7.1.3",
    "graceful-fs": "4.1.15",
    "rimraf": "^2.6.2",
    "underscore": "^1.8.3",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.6",
    "@types/node": "^9.4.5",
    "@types/underscore": "^1.8.7",
    "jasmine": "^2.9.0",
    "jest": "^22.1.4",
    "tslint": "~5.9.1",
    "tslint-microsoft-contrib": "~5.0.2",
    "tslint-react": "~3.4.0",
    "typescript": "~2.6.2"
  },
  "author": "hailiu@gmail.com",
  "license": "ISC"
}
